{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 3,
	"name": "k1-block-theme/k1-icon",
	"title": "K1 Icon Selector",
	"category": "k1-blocks",
	"description": "Grab a custom K1 icon",
	"textdomain": "kingdom_one",
	"supports": {
		"color": true,
		"dimensions": {
			"minHeight": true,
			"aspectRatio": false
		},
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"attributes": {
		"icon": {
			"type": "string",
			"default": "justice"
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
