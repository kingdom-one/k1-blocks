{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 3,
	"name": "k1-block-theme/k1-icon",
	"title": "K1 Icon Selector",
	"category": "k1-blocks",
	"description": "Grab a custom K1 icon",
	"textdomain": "kingdom_one",
	"supports": {
		"align": [
			"full"
		]
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"icon": {
			"type": "string",
			"default": "justice"
		},
		"iconColor": {
			"type": "string",
			"default": "white"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#dbdbdb"
		},
		"headline": {
			"type": "string",
			"default": "Meet a team member"
		},
		"headlineColor": {
			"type": "string",
			"default": "white"
		},
		"subheadline": {
			"type": "string"
		},
		"subheadlineColor": {
			"type": "string",
			"default": "white"
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}