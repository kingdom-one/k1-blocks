{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 3,
	"name": "k1-block-theme/k1-icon",
	"title": "K1 Icon Selector",
	"category": "k1-blocks",
	"description": "Grab a custom K1 icon",
	"textdomain": "kingdom_one",
	"supports": {
		"color": true,
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"attributes": {
		"icon": {
			"type": "string",
			"default": "justice"
		},
		"size": {
			"type": "number",
			"default": 100
		}
	},
	"style": "file:./style-index.css",
	"editorScript": "file:./index.tsx"
}
