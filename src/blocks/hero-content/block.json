{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 3,
	"name": "k1-block-theme/hero-content",
	"title": "Hero Content",
	"category": "text",
	"description": "The hero content",
	"textdomain": "kingdom_one",
	"attributes": {
		"headline": {
			"type": "string",
			"default": "the headline"
		},
		"subheadline": {
			"type": "string",
			"default": "the subheadline"
		},
		"textColor": {
			"type": "string",
			"default": "#ffffff"
		}
	},
	"supports": {
		"color": {
			"text": true,
			"background": false
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./index.css"
}