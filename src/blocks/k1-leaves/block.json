{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/block.json",
	"apiVersion": 3,
	"name": "k1-block-theme/k1-leaves",
	"title": "Leaves Selector",
	"category": "k1-blocks",
	"description": "Grab a leaves icon",
	"textdomain": "kingdom_one",
	"supports": {
		"color": {
			"text": true,
			"gradient": false
		},
		"typography": {
			"fontSizes": true
		},
		"dimensions": {
			"spacing": true,
			"minHeight": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"attributes": {
		"number": {
			"type": "string",
			"default": "3"
		},
		"direction": {
			"type": "string",
			"default": "left"
		},
		"size": {
			"type": "integer",
			"default": 50
		}
	},
	"style": "file:./style-index.css",
	"editorScript": "file:./index.tsx"
}
